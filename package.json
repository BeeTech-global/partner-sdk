{
  "name": "@beetech/partner-sdk",
  "version": "5.7.0",
  "description": "This repository holds all libs for partner use.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16.13.x",
    "npm": "8.1.x"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --forceExit --runInBand --silent --detectOpenHandles",
    "coverage": "jest test/ --coverage --forceExit --runInBand",
    "lint": "exit 0",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeeTech-global/partner-sdk.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BeeTech-global/partner-sdk/issues"
  },
  "homepage": "https://github.com/BeeTech-global/partner-sdk#readme",
  "devDependencies": {
    "@types/jest": "27.0.3",
    "faker": "5.5.3",
    "jest": "27.3.1",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.5.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coverageReporters": [
      "html",
      "json",
      "text-summary",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.(js|ts)"
    ]
  },
  "dependencies": {
    "bignumber.js": "^9.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
